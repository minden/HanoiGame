class Main {

    function void main() {
        var HanoiGame game;
        var boolean result;
        var boolean exit;

        let exit = false;


        while (~exit){

          let game = HanoiGame.new();
          let result = game.run();
          do game.dispose();
          if (~result){
            let exit = true;
          }
        }
        return;
    }
}
